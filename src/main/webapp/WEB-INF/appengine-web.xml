<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>patsformatters</application>
    <version>1</version>
    <threadsafe>true</threadsafe>
    <runtime>java8</runtime>

    <!-- Configure java.util.logging -->
    <system-properties>
        <property name="java.util.logging.config.file"
                  value="resources/logback.xml"/>
    </system-properties>

    <static-files>
       <include path="js/*.js"/>
       <include path="*.html"/>
       <include path="*.xml"/>
    </static-files>

</appengine-web-app>
